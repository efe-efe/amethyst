<root>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/util.js" />
        <include src="file://{resources}/scripts/custom_game/underscore-min.js" />
        <include src="file://{resources}/scripts/custom_game/status_bar_styles.js" />
        <include src="file://{resources}/scripts/custom_game/overhead.js" />
    </scripts>
    <styles>
        <include src="file://{resources}/styles/custom_game/overhead.css" />
    </styles>
    <snippets>

        <snippet name="ProgressBar">
            <Panel class="ProgressBar" hittest="false" hittestchildren="false">
                <Panel class="ProgressBar__Background" id="ProgressBar__Background" hittest="false" hittestchildren="false">
                    <Panel class="ProgressBar__Foreground" id="ProgressBar__Foreground" hittest="false" hittestchildren="false"/>
                </Panel>
            </Panel>
        </snippet>
    
        <snippet name="OverHero"> 
            <Panel class="OverHero" hittest="false" hittestchildren="false">


                <Panel class="OverHero__Top" hittest="false" hittestchildren="false">
                    <Panel id="Recast" class="Recast" hittest="false" hittestchildren="false" >
                        <Label id="Recast__Text" class="Recast__Text" html="true" text="R" />
                        <DOTAAbilityImage id="Recast__Image" class="Recast__Image" abilityname="" />
                    </Panel>
                </Panel>

                <Panel class="OverHero__Middle" hittest="false" hittestchildren="false">
                    <Panel id="Status" class="Status" hittest="false" hittestchildren="false" >
                        <Panel class="Status__Top" hittest="false" hittestchildren="false" >
                            <Label id="Status__Text--Glow" class="Status__Text--Glow" html="true" text="" />
                            <Label id="Status__Text--Normal" class="Status__Text--Normal" html="true" text="" />
                        </Panel>
                        <Panel class="Status__Bottom" id="Status__Bottom" hittest="false" hittestchildren="false"/>
                    </Panel>
                    <Panel id="Player" class="Player" hittest="false" hittestchildren="false" >
                        <Label id="Player__Text" class="Player__Text" html="true" text="No name" />
                    </Panel>
                </Panel>

                <Panel class="OverHero__Bottom" hittest="false" hittestchildren="false">
                    <Panel id="Charges" class="Charges" hittest="false" hittestchildren="false" >
                        <Label class="Charge__Text" html="true" text="(" />
                        <Label id="Charge__Text--Number" class="Charge__Text" html="true" text="0" />
                        <Label class="Charge__Text" html="true" text=")" />
                    </Panel>
                    
                    <Panel class="Resources" hittest="false" hittestchildren="false">
                        <Panel class="Resources__Health" hittest="false" hittestchildren="false">
                                <Panel id="Healthbar__Container--NoShield" class="Healthbar__Container--NoShield">
                                    <Panel class="Healthbar__Background" hittest="false" hittestchildren="false"/>
                                    <Panel class="Healthbar__Foreground" hittest="false" hittestchildren="false"/>   
                                </Panel>

                                <Panel id="Healthbar__Container--WithShield" class="Healthbar__Container--WithShield">
                                    <Panel id="Healthbar__Shield" class="Healthbar__Shield" hittest="false" hittestchildren="false" />
                                    <Panel class="Healthbar__Foreground" hittest="false" hittestchildren="false"/>                           
                                </Panel>
                        </Panel>

                        <Panel class="Resources__Mana" id="Resources__Mana" hittest="false" hittestchildren="false"/>
                        <Panel class="Resources__CastPoint" id="Resources__CastPoint" hittest="false" hittestchildren="false"/>
                    </Panel>

                    <Panel id="Stacks" class="Stacks" hittest="false" hittestchildren="false" >
                        <Panel class="Stacks__Stack" hittest="false" hittestchildren="false" />
                        <Panel class="Stacks__Stack" hittest="false" hittestchildren="false" />
                        <Panel class="Stacks__Stack" hittest="false" hittestchildren="false" />
                        <Panel class="Stacks__Stack" hittest="false" hittestchildren="false" />
                    </Panel>

                    <Panel id="Cooldown" class="Cooldown" hittest="false" hittestchildren="false" >
                        <Panel class="Cooldown__Background" hittest="false" hittestchildren="false" />
                        <Panel id="Cooldown__Foreground" class="Cooldown__Foreground" hittest="false" hittestchildren="false" />
                    </Panel>
                </Panel>
            </Panel>
        </snippet>

        
    </snippets>
    <Panel class="Overhead" hittest="false">
        <Panel id="OverheadContainer" hittest="false" class="Overhead__Container" />
    </Panel>
</root>



        